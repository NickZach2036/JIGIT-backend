{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["prettier", "unused-imports"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "unused-imports/no-unused-imports": "error",
      "prefer-const": "error"
    }
  }
  